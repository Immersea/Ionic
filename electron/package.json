{
  "name": "trasteel-app",
  "version": "1.4.0",
  "main": "main.js",
  "build": {
    "appId": "com.trasteel.app",
    "productName": "Trasteel",
    "files": [
      "main.js",
      "preload.js",
      {
        "from": "../www",
        "to": "www",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "output": "dist"
    },
    "extraResources": [
      {
        "from": "../www",
        "to": "resources/www"
      }
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "assets/icons/trasteel/icon512.png",
      "protocols": [
        {
          "name": "Trasteel Protocol",
          "schemes": [
            "trasteelapp"
          ]
        }
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icons/trasteel/favicon.ico",
      "protocols": [
        {
          "name": "Trasteel Protocol",
          "schemes": [
            "trasteelapp"
          ]
        }
      ]
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icons/trasteel/icon512.png",
      "protocols": [
        {
          "name": "Trasteel Protocol",
          "schemes": [
            "trasteelapp"
          ]
        }
      ]
    }
  },
  "scripts": {
    "start": "NODE_ENV=development electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "Immersea s.r.l.",
  "license": "ISC",
  "description": "Native Electron version of the app",
  "dependencies": {
    "http-server": "^14.1.1"
  },
  "devDependencies": {
    "electron": "^32.0.1",
    "electron-builder": "^25.0.5"
  }
}
